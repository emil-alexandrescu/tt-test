{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {

  },
  "rules": {
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "max-len": [2, 160, 4],
    "no-duplicate-imports": 0,
    "class-methods-use-this": 0,
    "new-cap": 0
  }
}
