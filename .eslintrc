{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "document": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "max-len": [2, 160, 4],
    "no-duplicate-imports": 0,
    "class-methods-use-this": 0,
    "new-cap": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "import/no-extraneous-dependencies": 0,
    "jsx-a11y/anchor-has-content": 0,
  }
}
